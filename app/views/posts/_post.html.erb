<div id="<%= dom_id post %>" class='post-container'>
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= post.description %>
  </p>

  <p>
    <strong>Keyword:</strong>
    <%= post.keyword %>
  </p>
  <p>
    <strong>Email:</strong>
    <%= post.user.email %>
  </p>
<p>
      <%= link_to "", post ,class:'post-link'%>
    </p>
  <% post.images.each do |image| %>
    <%= image_tag(image , style:"object-fit :cover; width: 250px; height: 600px;")  %>
  <% end %>



</div>
<p>
      <%= link_to post.user.email, post.user , style:'z-index:';%>
    </p>